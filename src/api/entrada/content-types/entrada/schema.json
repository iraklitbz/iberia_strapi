{
  "kind": "collectionType",
  "collectionName": "entradas",
  "info": {
    "singularName": "entrada",
    "pluralName": "entradas",
    "displayName": "Entrada"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "content": {
      "type": "richtext"
    },
    "excerpt": {
      "type": "text"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::entrada-category.entrada-category",
      "inversedBy": "entradas"
    },
    "acceso": {
      "type": "enumeration",
      "enum": ["publico", "miembros", "suscriptores"],
      "default": "publico"
    }
  }
}
