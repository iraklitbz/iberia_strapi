{
  "kind": "collectionType",
  "collectionName": "georgian_categories",
  "info": {
    "singularName": "georgian-category",
    "pluralName": "georgian-categories",
    "displayName": "Georgian Category"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "georgians": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::georgian.georgian",
      "mappedBy": "category"
    }
  }
}
